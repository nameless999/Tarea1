<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controlador.Session.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetAttrtibute</servlet-name>
        <servlet-class>Controlador.Session.SetAttrtibute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>closeSession</servlet-name>
        <servlet-class>Controlador.Session.closeSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getAttribute</servlet-name>
        <servlet-class>Controlador.Session.getAttribute</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetAttrtibute</servlet-name>
        <url-pattern>/SetAttrtibute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>closeSession</servlet-name>
        <url-pattern>/closeSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getAttribute</servlet-name>
        <url-pattern>/getAttribute</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
