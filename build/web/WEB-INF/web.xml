<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controlador.Session.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetAttrtibute</servlet-name>
        <servlet-class>Controlador.Session.SetAttrtibute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>closeSession</servlet-name>
        <servlet-class>Controlador.Session.closeSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getAttribute</servlet-name>
        <servlet-class>Controlador.Session.getAttribute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IngresarVendedor</servlet-name>
        <servlet-class>Controller.IngresarVendedor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IngresarCliente</servlet-name>
        <servlet-class>Controller.IngresarCliente</servlet-class>
    </servlet>
