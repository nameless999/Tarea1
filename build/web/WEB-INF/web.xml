    <servlet-mapping>
        <servlet-name>EditarProductos</servlet-name>
        <url-pattern>/EditarProductos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buscarProducto</servlet-name>
        <url-pattern>/buscarProducto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
