        <servlet-name>IngresarCompra</servlet-name>
        <servlet-class>Controller.IngresarCompra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateProducto</servlet-name>
        <servlet-class>Controller.updateProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet_1</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarProductos</servlet-name>
        <servlet-class>Controller.EditarProductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buscarProducto</servlet-name>
        <servlet-class>Controller.buscarProducto</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetAttrtibute</servlet-name>
        <url-pattern>/SetAttrtibute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>closeSession</servlet-name>
        <url-pattern>/closeSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getAttribute</servlet-name>
        <url-pattern>/getAttribute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresarVendedor</servlet-name>
        <url-pattern>/IngresarVendedor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresarCliente</servlet-name>
        <url-pattern>/IngresarCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresarProducto</servlet-name>
        <url-pattern>/IngresarProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresarCompra</servlet-name>
        <url-pattern>/IngresarCompra</url-pattern>
